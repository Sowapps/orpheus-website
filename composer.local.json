{
	"name": "orpheus/orpheus-website",
	"description": "Orpheus Website",
	"type": "project",
	"license": "MIT",
    "minimum-stability": "dev",
	"keywords": [
		"framework",
		"php"
	],
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
	"repositories": [
		{
			"type": "path",
			"url": "../*",
			"options": {
                "symlink": true
			}
		}
	],
	"require": {
        "php": ">=8.2",
        "orpheus/orpheus-core": "*",
		"orpheus/orpheus-cache": "*",
        "orpheus/orpheus-composer-plugin": "*",
		"orpheus/orpheus-yaml": "*",
		"orpheus/orpheus-initernationalization": "*",
		"orpheus/orpheus-rendering": "*",
		"orpheus/orpheus-inputcontroller": "*",
		"orpheus/orpheus-sqladapter": "*",
		"orpheus/orpheus-entitydescriptor": "*",
		"orpheus/orpheus-form": "*",
        "orpheus/orpheus-publisher": "*",
        "orpheus/orpheus-rest": "*",
        "orpheus/orpheus-ssh2": "*",
		"orpheus/orpheus-webtools": "*",
		"ext-pdo": "*",
        "ext-json": "*",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
	},
    "config": {
        "allow-plugins": {
            "orpheus/orpheus-composer-plugin": true
        }
    },
    "extra": {
        "orpheus-library": {
            "class": "App\\OrpheusApplicationLibrary"
		}
	},
	"authors": [
		{
			"name": "Florent HAZARD",
			"email": "contact@sowapps.com",
            "homepage": "https://sowapps.com",
			"role": "Developer"
		}
    ]
}

